extends layout

block content
    h1 Lista de Turmas
    p #{title}
    table(border =1)
     tr(align="center")
      td Escola
      td Ano
      td Nome
      td Ano Letivo
       -for (var id in items)
        - var item = items[id]
         tr
          td #{item.schoolName} 
          td #{item.classLevel} 
          td #{item.className}
          td #{item.classYear}
    a(href="/") Voltar